-- Name: sections_array_num_seq; Type: SEQUENCE; Schema: pierre; Owner: -
--

CREATE SEQUENCE sections_array_num_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


