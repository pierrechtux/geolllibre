--

COMMENT ON COLUMN licences.lat_min IS 'Minimum latitude';


--
-- Name: COLUMN licences.lon_min; Type: COMMENT; Schema: public; Owner: -
--

COMMENT ON COLUMN licences.lon_min IS 'Minimum longitude';


--
